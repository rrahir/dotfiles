// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "ctrl+[Quote]",
        "command": "workbench.panel.terminal.focus"
    },
    {
        "key": "ctrl+[Quote]",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "ctrl+shift+h",
        "command": "workbench.action.tasks.runTask",
        "args": "tig log"
    },
    {
        "key": "ctrl+shift+b",
        "command": "workbench.action.tasks.runTask",
        "args": "tig blame"
    }
]